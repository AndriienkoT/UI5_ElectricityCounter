{
  "_version": "0.0.1",
  "sap.app": {
    "_version": "0.0.1",
    "id": "UI5toLearn",
    "type": "application",
    "title": "UI5toLearn",
    "description": "Common project to develop GK Software specific libraries, components and controls for UI5 applications",
    "applicationVersion": {
      "version": "0.0.1"
    }
  },
  "sap.ui": {
    "_version": "1.50.7",
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_belize"
    ]
  },
  "sap.ui5": {
    "_version": "0.0.1",
    "rootView": {
      "viewName": "UI5toLearn.view.App",
      "type": "XML",
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.44",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "Model": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/Model.json",
        "preload": true
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ],
      "js": [

      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "UI5toLearn.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "transition": "slide",
        "homeRoute": "login"
      },
      "routes": [
        {
          "pattern": "",
          "name": "login",
          "target": "login"
        },
        {
          "pattern": "main",
          "name": "main",
          "target": "main"
        }
      ],
      "targets": {
        "login": {
          "viewId": "login",
          "viewName": "Login",
          "viewLevel": 1
        },
        "main": {
          "viewName": "Main",
          "viewLevel": 1
        }
      }
    }
  }
}
