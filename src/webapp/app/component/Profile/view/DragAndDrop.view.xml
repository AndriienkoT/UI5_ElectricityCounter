<mvc:View
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:core="sap.ui.core"
  xmlns="sap.m"
  xmlns:tnt="sap.tnt"
  controllerName="UI5toLearn.component.Profile.controller.DragAndDrop" width="100%" height="100%"
  displayBlock="true">

  <tnt:ToolPage id="dragAndDrop">
    <tnt:header>
      <tnt:ToolHeader class="customHeader">
        <core:Icon src="sap-icon://nav-back" press="onNavBack" class="customReturnIcon"/>
        <Text text="Drag and drop" class="customHeaderText" />
      </tnt:ToolHeader>
    </tnt:header>

    <tnt:mainContents>
      <Button id="movableButton" text="Movable button" />
    </tnt:mainContents>
  </tnt:ToolPage>

</mvc:View>
