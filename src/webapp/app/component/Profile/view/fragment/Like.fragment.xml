<core:FragmentDefinition
  xmlns:core="sap.ui.core"
  xmlns="sap.m">
  <SelectDialog title="Members you like"
                type="Active"
                items="{path: '/members'}"
                search="handleSearch"
                cancel="handleClose"
                confirm="handleClose">
      <StandardListItem
        title="{name}"
        icon="{image}"
        iconDensityAware="false"
        iconInset="false"
        type="Active" />
  </SelectDialog>

</core:FragmentDefinition>
